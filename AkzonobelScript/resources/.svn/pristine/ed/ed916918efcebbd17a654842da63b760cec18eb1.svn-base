/**
 * Use this file for the colors and graphics styles.
 *
 */

body,
.layout-caption,
.layout-content
{
    background-color : #d3d3d3;
}

.layout-side
{
    background-color : #d3d3d3;
}

.layout-caption
{
    color: #282828;
}

.layout-tools
{
    background-color : silver;
}

.layout-tools-side
{
    background-color : #d3d3d3;
}

.layout-tools-content
{
    background  : url("images/background-black-75.png");
}

.application-logo
{
    background: url("images/logo.png") #d3d3d3 no-repeat center center;
    width: 100px;
    height: 19px;
}

.application-user .user-label
{
    color: #d3d3d3;
}

.application-user .user-name
{
    color: white;
}

.stream-video-header
{
    background : url("images/background-black-46.png");
}

.stream-video-control-container, .stream-video-control
{
    background : url("images/icon-no-video.png") no-repeat;
}

.stream-video-control-client-container, .stream-video-control-client
{
    background : url("images/video-not-broadcasting.png") no-repeat;
}

.stream-video-background-none{
    background-image: none;
}

.video-no-plug
{
    background: none;
}


.icon-user
{
    background : url("images/icon-user.png") no-repeat;
}

.icon-user-own
{
    background : url("images/icon-user-own.png") no-repeat;
}

.icon-video-disabled
{
    background : url("images/icon-video-enabled.png") no-repeat;
}

.icon-video-enabled
{
    background : url("images/icon-video-disabled.png") no-repeat;
}

.bgInner{
    background: url('images/bgInner.png') repeat-x #d3d3d3;
}
.authInput{ 
    background: url('images/bgInput.png') no-repeat;
    color: #282828;
}
.sessionInput{
    background: url('images/sessionInput.png') no-repeat;
    color: #282828;
}
.inputLarge{
    background: url('images/bgInput1.png') no-repeat;
    color: #282828;
}
.inputShort{
    background: url('images/bgInputShort1.png') no-repeat;
    color: #282828;
}
.logo{
    background: url('images/logo.png') no-repeat;
}
.select-shadow {
    background:url('images/select-background.png') no-repeat bottom right;
}
.bgTopCenter{
    background: url('images/page_topCenter.png') repeat-x;
}
.bgMiddleLeft{
    background: url('images/page_middleLeft.png') repeat-y right;
}
.middleRight{
    background: url('images/page_middleRight.png') repeat-y;
}
.bgBottomCenter{
    background: url('images/page_bottomCenter.png') repeat-x;
}



.bgBottomLeft{
    background: url('images/page_bottomLeft.png') no-repeat;
}
.bgBottomRight{
    background: url('images/page_bottomRight.png') no-repeat;
}
.bgTopLeft{
    background: url('images/page_topLeft.png') repeat-y;
    width: 11px;
}
.bgTopRight{
    background: url('images/page_topRight.png') repeat-y;
    width: 11px;
}

/*
 * Messages
 */

.in-message-border-topleft { background : url("images/history/in-border-topleft.png") no-repeat; }
.in-message-border-top { background : url("images/history/in-border-topcenter.png") repeat-x bottom; }
.in-message-border-topright { background : url("images/history/in-border-topright.png") no-repeat; }
.in-message-border-middleleft { background : url("images/history/in-border-middleleft.png") repeat-y; }
.in-message-border-middle { background : #e9e9e9; }
.in-message-border-middleright { background : url("images/history/in-border-middleright.png") repeat-y; }
.in-message-border-bottomleft { background : url("images/history/in-border-bottomleft.png") no-repeat; }
.in-message-border-bottom { background : url("images/history/in-border-bottomcenter.png") repeat-x bottom; }
.in-message-border-bottomright { background : url("images/history/in-border-bottomright.png") no-repeat; }

.in-message-dialog-arrow { background : url("images/history/in-border-dialog-arrow.png") no-repeat; }

.in-message-username { color : #989797; }
.in-message-text, .in-message-text a { color : #0a0a0a; text-decoration : none; }

/*----------*/

.out-message-border-topleft { background : url("images/history/out-border-topleft.png") no-repeat; }
.out-message-border-top { background : url("images/history/out-border-topcenter.png") repeat-x bottom; }
.out-message-border-topright { background : url("images/history/out-border-topright.png") no-repeat; }
.out-message-border-middleleft { background : url("images/history/out-border-middleleft.png") repeat-y; }
.out-message-border-middle { background : #fefefe; }
.out-message-border-middleright { background : url("images/history/out-border-middleright.png") repeat-y; }
.out-message-border-bottomleft { background : url("images/history/out-border-bottomleft.png") no-repeat; }
.out-message-border-bottom { background : url("images/history/out-border-bottomcenter.png") repeat-x bottom; }
.out-message-border-bottomright { background : url("images/history/out-border-bottomright.png") no-repeat; }

.out-message-dialog-arrow { background : url("images/history/out-border-dialog-arrow.png") no-repeat; }

.out-message-username { color : #e57665; }
.out-message-text, .out-message-text a { color : #c22912; text-decoration : none; }

.image-preview
{
    -webkit-box-shadow : 0px 0px 5px rgba(0, 0, 0, 0.6);
    -moz-box-shadow    : 0px 0px 5px rgba(0, 0, 0, 0.6);
    box-shadow         : 0px 0px 5px rgba(0, 0, 0, 0.6);

    background : #FEFEFE;
}

