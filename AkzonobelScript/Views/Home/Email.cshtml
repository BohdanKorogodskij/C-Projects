        <script>
            $("#btnSend").click(function () {
                var IsCancel = $("#IsCancel").prop("checked");
                var Address = $("#Address").val();
                var Fio = $("#Fio").val();
                var FioTxt = $("#FioTxt").val();
                var Company = $("#Company").val();
                var CompanyTxt = $("#CompanyTxt").val();
                var Phone = $("#Phone").val();
                var Message = $("#Message").val();
                $.ajax({
                    contentType: 'application/json',
                    type: "POST",
                    url: '@Url.Action("EmailSend", "Home", new { Area = ""})',
                    data: JSON.stringify({
                        IsCancel: IsCancel,
                        Address: Address,
                        Fio: Fio,
                        FioTxt: FioTxt,
                        Company: Company,
                        CompanyTxt: CompanyTxt,
                        Phone: Phone,
                        Message: Message
                    })
                });
            });
